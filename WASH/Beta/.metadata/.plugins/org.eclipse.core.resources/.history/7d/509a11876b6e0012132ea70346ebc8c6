package com.kevingil.slidelistviewheader;

public class WeatherHeader {
    public int icon;
    public String title;
    public String titleheader;
    
    public Weather(){
        super();
    }
    public WeatherHeader(){
        super();
    }
    
    public Weather(int icon, String title) {
        super();
        this.icon = icon;
        this.title = title;
    }
    public WeatherHeader(String titleheader) {
        super();
        this.titleheader = titleheader;
    }
}
